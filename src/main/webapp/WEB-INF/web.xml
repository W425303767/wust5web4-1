  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Registe</servlet-name>
    <url-pattern>/servlet/Registe</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
